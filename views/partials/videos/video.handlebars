<!-- events front end template (remember \{{ }} ) -->
<script id="videoTemplate" type="text/x-andlebars-template">

	<div class="video" id="\{{ id }}">
		<div class="usermeta">
			<header>
				<div class="closeBtn"></div>
				<div class="mapBtn"></div>
				<div class="username">\{{ name }}</div>
			</header>
			<div class="mapWrap">
				<div class="locactionName">Map</div>
				<div class="map"></div>
			</div>
		</div>
		
		<video class="video" \{{#if mute }} muted \{{/if}} autoplay="true"></video>
	</div>

</script>